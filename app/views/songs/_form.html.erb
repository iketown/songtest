<%= simple_form_for(@song) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :embed_code, as: :text %>
    <%= f.input :bandcamp_id %>
    <%= f.input :artist_id %>

    <%= f.input :album_number %>
    <%= f.input :track_number %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
